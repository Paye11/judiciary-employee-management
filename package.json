{
  "name": "judiciary-staff-management",
  "version": "1.0.0",
  "description": "Complete Judiciary Staff Management System with Frontend and Backend",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "frontend": "cd frontend && npm start",
    "backend": "cd backend && npm start",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "build": "echo 'Build process - ready for deployment'",
    "test": "echo 'No tests specified yet'"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "keywords": [
    "judiciary",
    "staff-management",
    "nodejs",
    "express",
    "fullstack"
  ],
  "author": "Judiciary Staff Management Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/judiciary-staff-management.git"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}